{
  "name": "improved-pokequiz",
  "version": "1.0.0",
  "description": "trivia game with pokemon",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDb.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crispysodium/improved-pokequiz.git"
  },
  "author": "crispysodium, setocourtney",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crispysodium/improved-pokequiz/issues"
  },
  "homepage": "https://github.com/crispysodium/improved-pokequiz#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv": "^5.3.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-sequelize": "^0.9.0",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
